{
    "builds": [{
        "src": "ApiProject1/ApiProject/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python 3.9" }
    }],

    "routes": [
        {
            "src": "/(.*)",
            "dest": "ApiProject1/ApiProject/wsgi.py"
        }
    ]
}